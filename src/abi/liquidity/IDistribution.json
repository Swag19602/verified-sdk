  "source": "// Interface of Verified Liquidity distribution\r\n// (c) Kallol Borah, 2021\r\n\r\n//\"SPDX-License-Identifier: UNLICENSED\"\r\n\r\npragma solidity 0.8.0;\r\n\r\ninterface IDistribution {\r\n\r\n    function getPaymentFeeCollected(bytes32 _currency) external returns(uint256);\r\n\r\n    function getLoanFeeCollected(bytes32 _currency) external view returns(uint256);\r\n\r\n    function addRevenueShareholder(bytes32 _type, address _shareholder, bytes32 _currency) external;\r\n\r\n    function getRevenueShareholders(bytes32 _type, bytes32 _currency) external view returns(address[] memory shareholders);\r\n\r\n    function shareFee() external;\r\n\r\n}",
