  "source": "// (c) Kallol Borah, 2020\r\n// Interface definition of the Via bond token.\r\n// SPDX-License-Identifier: MIT\r\n\r\npragma solidity 0.5.7;\r\n\r\ninterface ViaBond{\r\n\r\n    function convert(bytes32 txId, bytes16 result, bytes32 rtype) external;\r\n\r\n    function transferForward(address _sender, address _receiver, uint256 _tokens, address _forwarder, bytes32 _symbol) external returns (bool);\r\n\r\n    function requestIssue(bytes16 amount, address payer, bytes32 currency, address cashContract) external returns(bool);\r\n\r\n    //function getBondIssues() external view returns(address[] memory, uint256[] memory);\r\n\r\n    //function getBondPurchases() external view returns(address[] memory, uint256[] memory);\r\n\r\n}",
