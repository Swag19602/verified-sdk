  "source": "// (c) Kallol Borah, 2020\r\n// Interface of the Via cash token.\r\n// SPDX-License-Identifier: MIT\r\n\r\npragma solidity 0.5.7;\r\n\r\ninterface ViaCash{\r\n\r\n    struct token{\r\n        bytes32 tokenName;\r\n        address tokenAddress;\r\n    }\r\n\r\n    function supportTokens(address _token, bytes32 _name) external;\r\n\r\n    function checkSupportForToken(address _token) external view returns(bool);\r\n\r\n    function buy(address _token, uint256 _amount, address _buyer) external;\r\n\r\n    function redeemDeposits(uint256 amount, address redeemer, bytes32 currency) external;\r\n\r\n    /*function depositCollateral(uint256 amount, address sender, bytes32 currency) external;\r\n\r\n    function addIssuedBalance(uint256 amount, address buyer, bytes32 currency) external;\r\n\r\n    function transferBalance(address transferor, uint256 amount, address transferee, bytes32 currency) external;\r\n\r\n    function transferDeposit(address transferor, uint256 amount, address transferee, bytes32 currency) external;\r\n\r\n    function redeemIssuedBalance(uint256 amount, address redeemer, bytes32 currency) external;*/\r\n\r\n}",
